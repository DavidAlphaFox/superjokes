
@{
    ViewBag.BgClass = "indexPage-body";
    Layout = "~/Views/Shared/_BaseLayoutPage.cshtml";
    Joke.Common.LogHelper.Info("islogin:"+Request.IsAuthenticated);

    UserInfoPrincipal userinfo = System.Web.HttpContext.Current.User as UserInfoPrincipal;
    if(userinfo!=null)
    {
        Joke.Common.LogHelper.Info("userinfo:" + userinfo.UserName + ",isadmin:" + userinfo.IsAdmin);
    }
    
}
@using Joke.Web.Auth
@section header{
  <script type="text/javascript" src="~/Scripts/jquery.lazyload.min.js"></script>
    <script type="text/javascript">
        $(function () {
            $("img.lazyloading").lazyload();
        });
    </script>
}
<div class="wrapper clearfix">
    <!-- main -->
    <div class="main">
        <!-- section -->
        @Html.Action("CategorySummary","Home",null)
        <!-- //section -->
        <!-- new-panel -->
        @Html.Action("LatestJokes", "Home", null)
        <!-- //new-panel -->
    </div>
    <!-- //main -->
    <!-- aside -->
    <div class="aside">
        <!-- section -->
        @Html.Action("LikeMostJokes", "Home", null)
        <!-- //section -->
    </div>
    <!-- //aside -->
</div>

